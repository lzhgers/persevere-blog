<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lzh.lzhframework.dao.ExceptionLogMapper">

    <select id="queryTodayIpNum" resultType="java.lang.String">
        select ip
        from lzh_exception_log
        where del_flag = 0
          and date (create_time) = #{date}
    </select>
</mapper>